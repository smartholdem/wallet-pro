{
  "name": "smartholdem-wallet-pro",
  "version": "1.2.19",
  "description": "SmartHoldem Wallet Pro is a desktop wallet (Electron) for the SmartHoldem network with cross-chain support",
  "author": "SmartHoldem Team",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build:web": "vue-tsc --noEmit && vite build",
    "build:android-release": "npm run build:cordova && cd src-cordova && cordova build android --release --buildConfig=build.json --packageType=apk && ren platforms\\android\\app\\build\\outputs\\apk\\release\\app-release.apk smartholdem-wallet-pro-android-1.2.19.apk",
    "build:extension": "vue-tsc --noEmit && vite build -c vite.config.extension.ts",
    "build:electron": "vite build --base=./ && electron-builder -w",
    "build:electron:linux": "vite build --base=./ && electron-builder -l",
    "build:electron:mac": "vite build --base=./ && electron-builder -m",
    "build:android-debug": "npm run build:cordova && cd src-cordova && cordova build android",
    "build:cordova": "vue-tsc --noEmit && vite build -c vite.config.cordova.ts",
    "build:chrome": "vue-tsc --noEmit && vite build -c vite.config.extension.chrome.ts",
    "electron:dev": "concurrently -k \"vite --host  0.0.0.0 --port 5173\" \"wait-on http://localhost:5173 && npx electron electron/main.js\"",
    "host": "vite --host 0.0.0.0",
    "electron:manual": "npx electron electron/main.js",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@caohenghu/vue-colorpicker": "^1.2.4",
    "@chenfengyuan/vue-countdown": "^2.1.3",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@highlightjs/vue-plugin": "github:highlightjs/vue-plugin",
    "@marcoschulte/vue3-progress": "^0.0.7",
    "@popperjs/core": "^2.11.8",
    "@sipec/vue3-tags-input": "^3.0.4",
    "@smartholdem/client": "^2.0.0",
    "@smartholdem/crypto": "^3.8.3",
    "@types/es6-promise": "^3.3.0",
    "@vuepic/vue-datepicker": "^5.3.0",
    "@vueup/vue-quill": "^1.2.0",
    "@vueuse/core": "^10.4.1",
    "apexcharts": "^5.2.0",
    "axios": "1.12.2",
    "bip39": "^3.0.4",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "buffer": "^6.0.3",
    "chart.js": "^4.3.0",
    "crypto-js": "4.2.0",
    "crypto-random-string": "^5.0.0",
    "datatables.net-bs5": "^1.13.4",
    "datatables.net-buttons": "^2.3.6",
    "datatables.net-buttons-bs5": "^2.3.6",
    "datatables.net-fixedcolumns": "^4.2.2",
    "datatables.net-fixedcolumns-bs5": "^4.2.2",
    "datatables.net-responsive": "^2.4.1",
    "datatables.net-responsive-bs5": "^2.4.1",
    "datatables.net-vue3": "^2.1.1",
    "emittery": "^1.0.1",
    "fabric": "^5.5.2",
    "highlight.js": "^11.11.1",
    "jquery": "^3.7.0",
    "jsvectormap": "^1.7.0",
    "jszip": "^3.10.1",
    "lity": "^2.4.1",
    "lodash": "^4.17.21",
    "marked": "^16.3.0",
    "maska": "3.2.0",
    "masonry-layout": "^4.2.2",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "pdfmake": "^0.2.20",
    "photoswipe": "^5.4.4",
    "picmo": "^5.8.5",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.2.0",
    "print-js": "^1.6.0",
    "process": "^0.11.10",
    "qrcode.vue": "^3.4.1",
    "qrious": "^4.0.2",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "semver": "^7.7.3",
    "vue": "^3.5.17",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^11.1.12",
    "vue-qrcode-reader": "^5.7.3",
    "vue-router": "^4.2.2",
    "vue-select": "^4.0.0-beta.3",
    "vue3-apexcharts": "^1.8.0",
    "vue3-datepicker": "^0.4.0",
    "vue3-perfect-scrollbar": "^1.6.1",
    "vue3-simple-typeahead": "^1.0.11",
    "vue3-table-lite": "^1.3.7",
    "web3": "^4.1.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/crypto-js": "^4.2.2",
    "@types/jsdom": "^21.1.7",
    "@types/marked": "^5.0.2",
    "@types/node": "^24.9.1",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^8.2.2",
    "cypress": "^15.3.0",
    "electron": "^29.4.0",
    "electron-builder": "^24.13.3",
    "eslint": "^8.43.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-vue": "^9.15.0",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "sass": "1.63",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.9.2",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^0.17.0",
    "vitest": "^0.32.2",
    "vue-tsc": "^3.0.8",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.smartholdem.walletpro",
    "productName": "SmartHoldem Wallet Pro",
    "artifactName": "${name}-${os}-${arch}-${version}.${ext}",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "asar": true,
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "snap"
      ],
      "category": "Finance",
      "icon": "build/icons"
    },
    "snap": {
      "grade": "stable",
      "confinement": "strict"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.finance",
      "icon": "build/icon.icns"
    },
    "extraMetadata": {
      "main": "electron/main.js"
    }
  }
}
