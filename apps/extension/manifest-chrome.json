{
  "manifest_version": 2,
  "name": "SmartHoldem Wallet",
  "version": "1.2.19",
  "description": "SmartHoldem cryptocurrency wallet browser extension",
  "permissions": [
    "storage",
    "activeTab",
    "https://*.smartholdem.io/*",
	"https://node0.smartholdem.io/*",
    "https://explorer.smartholdem.io/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SmartHoldem Wallet",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}